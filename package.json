{
  "name": "foundationlabs",
  "private": true,
  "version": "1.0.0",
  "description": "News feeds test",
  "main": "app.js",
  "nyc": {
    "check-coverage": true,
    "lines": 55,
    "statements": 55,
    "functions": 80,
    "branches": 55,
    "cache": true,
    "all": true,
    "ignore-class-method": "methodToIgnore",
    "extension": [
      ".test.js"
    ],
    "include": [
      "server/src/**/*.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "report-dir": "./coverage/server-coverage/"
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "setupFiles": [
      "./client/src/tests/jestsetup.js"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.jsx?$",
    "coverageThreshold": {
      "global": {
        "branches": 55,
        "functions": 80,
        "lines": 55,
        "statements": 55
      }
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "client/src/**/*.{js,jsx}",
      "!client/src/main.js",
      "!client/src/tests/**/*.js"
    ],
    "coverageDirectory": "./coverage/client-coverage/",
    "coverageReporters": [
      "lcov",
      "html",
      "text-summary"
    ],
    "roots": [
      "<rootDir>/client/src"
    ]
  },
  "scripts": {
    "test": "npm run lint && npm run test-server && npm run test-client",
    "lint": "npm run lint-client && npm run lint-server",
    "lint-client": "eslint ./client/src/**/*.js --quiet",
    "lint-server": "eslint ./server/src/**/*.js --quiet",
    "test-client": "jest -u --coverage",
    "test-server": "nyc mocha --opts ./mocha-server.opts",
    "test-server-unit": "nyc mocha --opts ./mocha-server-unit.opts",
    "test-server-integration": "nyc mocha --opts ./mocha-server-integration.opts",
    "start": "npm run build && node ./server/src/server.js && echo newsfeed",
    "build": "webpack",
    "watch": "webpack --watch"
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "engines": {
    "node": ">=8.11.3",
    "npm": "6.1.0"
  },
  "author": "Diego Castro",
